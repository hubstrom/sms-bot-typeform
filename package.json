{
  "name": "sms-bot-typeform",
  "version": "0.1.0",
  "description": "SMS bot for typeform",
  "author": "Laurynas Zavadskis <pastor.pit@protonmail.ch>",
  "scripts": {
    "start": "yarn dev:start",
    "dev:start": "nodemon --ignore lib --exec babel-node src/server",
    "prod:build": "rimraf lib && babel src -d lib --ignore .test.js",
    "prod:start": "cross-env NODE_ENV=production pm2 start lib/server && pm2 logs",
    "prod:stop": "pm2 delete server",
    "test": "eslint src && flow && jest --coverage",
    "precommit": "eslint src && flow",
    "prepush": "eslint src && flow && yarn prod:build"
  },
  "main": "lib/server/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/hitchnsmile/sms-bot-typeform"
  },
  "bugs": {
    "url": "https://github.com/hitchnsmile/sms-bot-typeform/issues"
  },
  "keywords": [
    "twilio",
    "typeform",
    "bot"
  ],
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "body-parser": "^1.17.1",
    "cron": "^1.2.1",
    "csv-write-stream": "^2.0.0",
    "debug": "~2.6.0",
    "express": "~4.14.1",
    "moment": "^2.18.1",
    "pg": "^6.1.5",
    "request": "^2.81.0",
    "twilio": "^3.0.0",
    "typeform-node-api": "^1.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.2",
    "babel-jest": "^19.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-flow": "^6.23.0",
    "babel-runtime": "^6.23.0",
    "cross-env": "^4.0.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-compat": "^1.0.2",
    "eslint-plugin-flowtype": "^2.32.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2 || ^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "flow-bin": "^0.44.2",
    "jest": "^19.0.2",
    "nodemon": "^1.11.0",
    "pm2": "^2.4.5",
    "rimraf": "^2.6.1"
  },
  "private": true
}
